<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ContentTextPrimaryLevel4">
        <item name="android:textSize">@dimen/text_size_l4</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>
    <style name="ContentTextBlackLevel4">
        <item name="android:textSize">@dimen/text_size_l4</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="ContentTextBlackBoldLevel4">
        <item name="android:textSize">@dimen/text_size_l4</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ContentTextBlackBoldLevel6">
        <item name="android:textSize">@dimen/text_size_l6</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ContentTextPrimaryBoldLevel6">
        <item name="android:textSize">@dimen/text_size_l6</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ContentTextBlackBoldLevel8">
        <item name="android:textSize">@dimen/text_size_l8</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ContentTextPrimaryBoldLevel4" parent="ContentTextPrimaryLevel4">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="editTextStyle">
        <item name="hintTextColor">@color/gray</item>
        <item name="android:background">@drawable/round_fill_white_background</item>
    </style>

    <style name="ContentTextWhiteLevel4">
        <item name="android:textSize">@dimen/text_size_l4</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
</resources>